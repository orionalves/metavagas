{
  "compilerOptions": {
    "target": "es2022",
    "module": "commonjs",
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "@/*": [ "./src/*" ],
      "@/auth/*": [ "./src/app/auth/*" ],
      "@/city/*": [ "./src/app/city/*" ],
      "@/citySearch/*": [ "./src/app/citySearch/*" ],
      "@/job/*": [ "./src/app/job/*" ],
      "@/user/*": [ "./src/app/user/*" ],
      "@/technology/*": [ "./src/app/technology/*" ],
      "@/techSearch/*": [ "./src/app/techSearch/*" ],
    },
    "types": [ "vitest/globals" ]
  }
}